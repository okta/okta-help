<?xml version="1.0" encoding="utf-8"?>
<CatapultSkin
  Version="3"
  SkinType="Component"
  Comment="HTML5 Toolbar skin"
  ComponentType="TopicToolbar"
  EnableResponsiveOutput="True"
  conditions="MultiProdPubish.ProjectLink">
  <Stylesheet>
    <Resources>
      <Resource
        Name="globe-23.png"></Resource>
      <Resource
        Name="globe-19.png"></Resource>
    </Resources>
    <Styles>
      <Style
        Name="ToolbarItem">
        <Classes>
          <StyleClass
            Name="SelectLanguage">
            <Properties>
              <Property
                Name="PaddingRight">15px</Property>
              <Property
                Name="BackgroundImage">url('globe-19.png')</Property>
              <Property
                Name="BackgroundGradient">vertical #ffffff #ffffff</Property>
              <Property
                Name="BackgroundColor">#ffffff</Property>
              <Property
                Name="BorderLeft">none 1px #e1e1e1</Property>
              <Property
                Name="BorderRight">none 1px #e1e1e1</Property>
              <Property
                Name="BorderTop">none 1px #e1e1e1</Property>
              <Property
                Name="BorderBottom">none 1px #e1e1e1</Property>
            </Properties>
          </StyleClass>
        </Classes>
      </Style>
    </Styles>
  </Stylesheet>
  <TopicToolbar
    EnableCustomLayout="true"
    Buttons="SelectLanguage|PreviousTopic|NextTopic" />
  <Strings>
    <Stylesheet
      lang="en-us">
      <Stylesheet>
        <Styles>
          <Style
            Name="ToolbarItem">
            <Classes>
              <StyleClass
                Name="NextTopic">
                <Properties>
                  <Property
                    Name="Tooltip">Next topic</Property>
                </Properties>
              </StyleClass>
              <StyleClass
                Name="PreviousTopic">
                <Properties>
                  <Property
                    Name="Tooltip">Previous topic</Property>
                </Properties>
              </StyleClass>
            </Classes>
          </Style>
        </Styles>
      </Stylesheet>
    </Stylesheet>
    <Stylesheet
      lang="ja-JP">
      <Stylesheet>
        <Styles>
          <Style
            Name="ToolbarItem">
            <Classes>
              <StyleClass
                Name="NextTopic">
                <Properties>
                  <Property
                    Name="Tooltip">次へ</Property>
                </Properties>
              </StyleClass>
              <StyleClass
                Name="PreviousTopic">
                <Properties>
                  <Property
                    Name="Tooltip">前へ</Property>
                </Properties>
              </StyleClass>
            </Classes>
          </Style>
        </Styles>
      </Stylesheet>
    </Stylesheet>
  </Strings>
</CatapultSkin>